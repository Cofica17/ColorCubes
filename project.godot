[application]

config/name="Color Cube"
run/main_scene="res://levels_creator/LevelsCreator.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://singletons/Global.gd"
BoardThemes="*res://board_themes/BoardThemes.gd"
Utils="*res://singletons/Utils.gd"
CheckSolution="*res://singletons/CheckSolution.gd"
Levels="*res://singletons/Levels.gd"
LevelsCreatorConfiguration="*res://levels_creator/LevelsCreatorConfiguration.gd"

[display]

window/size/width=720
window/size/height=1280
window/size/always_on_top=true
window/size/test_width=400
window/size/test_height=800
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[input]

swipe={
